<?xml version="1.0" encoding="ISO-8859-1"?>

<project>
  <extend>${basedir}/../etc/project.xml</extend>

  <id>distribution</id>

  <dependencies>

    <dependency>
      <groupId>openjms</groupId>
      <artifactId>openjms-common</artifactId>
      <url>http://openjms.sourceforge.net</url>
      <properties>
        <client.dependency.runtime>true</client.dependency.runtime>
      </properties>
    </dependency>

    <dependency>
      <groupId>openjms</groupId>
      <artifactId>openjms-net</artifactId>
      <url>http://openjms.sourceforge.net</url>
      <properties>
        <client.dependency.runtime>true</client.dependency.runtime>
      </properties>
    </dependency>

    <dependency>
      <groupId>openjms</groupId>
      <artifactId>openjms-tools</artifactId>
      <url>http://openjms.sourceforge.net</url>
      <properties>
      </properties>
    </dependency>

    <dependency>
      <groupId>openjms</groupId>
      <artifactId>openjms-tunnel</artifactId>
      <type>war</type>
      <url>http://openjms.sourceforge.net</url>
    </dependency>

    <dependency>
      <groupId>openjms</groupId>
      <artifactId>openjms</artifactId>
      <url>http://openjms.sourceforge.net</url>
      <properties>
        <client.classpath>true</client.classpath>
        <client.dependency.compile>true</client.dependency.compile>
      </properties>
    </dependency>

    <dependency>
      <groupId>antlr</groupId>
      <artifactId>antlr</artifactId>
      <url>http://www.antlr.org/</url>
    </dependency>

    <dependency>
      <groupId>castor</groupId>
      <artifactId>castor</artifactId>
      <url>http://castor.exolab.org/</url>
    </dependency>

    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <url>http://jakarta.apache.org/commons/collections/</url>
    </dependency>

    <dependency>
      <groupId>commons-codec</groupId>
      <artifactId>commons-codec</artifactId>
      <url>http://jakarta.apache.org/commons/codec</url>
      <properties>
        <client.dependency.runtime>true</client.dependency.runtime> 
     </properties>
    </dependency>

    <dependency>
      <groupId>commons-dbcp</groupId>
      <artifactId>commons-dbcp</artifactId>
      <url>http://jakarta.apache.org/commons/dbcp/</url>
    </dependency>

    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <url>http://jakarta.apache.org/commons/logging/</url>
      <properties>
        <client.dependency.runtime>true</client.dependency.runtime>
      </properties>
    </dependency>

    <dependency>
      <groupId>commons-pool</groupId>
      <artifactId>commons-pool</artifactId>
      <url>http://jakarta.apache.org/commons/pool/</url>
    </dependency>

    <dependency>
      <groupId>concurrent</groupId>
      <artifactId>concurrent</artifactId>
      <url>http://gee.cs.oswego.edu/dl/classes/EDU/oswego/cs/dl/util/concurrent/</url>
      <properties>
        <client.dependency.runtime>true</client.dependency.runtime>
      </properties>
    </dependency>

    <dependency>
      <groupId>org.apache.derby</groupId>
      <artifactId>derby</artifactId>
      <url>http://db.apache.org/derby</url>
    </dependency>

    <dependency>
      <groupId>jdbc</groupId>
      <artifactId>jdbc</artifactId>
      <url>http://java.sun.com/products/jdbc</url>
    </dependency>

    <dependency>
      <groupId>jms</groupId>
      <artifactId>jms</artifactId>
      <url>http://java.sun.com/products/jms</url>
      <properties>
        <client.dependency.compile>true</client.dependency.compile>
        <client.dependency.runtime>true</client.dependency.runtime>
      </properties>
    </dependency>

    <dependency>
      <groupId>jndi</groupId>
      <artifactId>jndi</artifactId>
      <url>http://java.sun.com/products/jndi</url>
      <properties>
        <category>lib</category>
        <client.dependency.compile>true</client.dependency.compile>
        <client.dependency.runtime>true</client.dependency.runtime>
      </properties>
    </dependency>

    <dependency>
      <groupId>jta</groupId>
      <artifactId>jta</artifactId>
      <url>http://java.sun.com/products/jta</url>
    </dependency>

    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <url>http://logging.apache.org/log4j/</url>
    </dependency>

    <dependency>
      <groupId>oro</groupId>
      <artifactId>oro</artifactId>
      <url>http://jakarta.apache.org/oro/</url>
    </dependency>

    <dependency>
      <groupId>spice</groupId>
      <artifactId>spice-jndikit</artifactId>
      <url>http://spice.codehaus.org/</url>
      <properties>
        <client.dependency.runtime>true</client.dependency.runtime> 
     </properties>
    </dependency>

    <dependency>
      <groupId>xml-apis</groupId>
      <artifactId>xml-apis</artifactId>
      <url>http://xml.apache.org/xerces2-j/</url>
    </dependency>

    <dependency>
      <groupId>xerces</groupId>
      <artifactId>xerces</artifactId>
      <url>http://xml.apache.org/xerces2-j/</url>
    </dependency>

    <!-- velocity dependencies for xdoc processing -->
    <dependency>
      <groupId>velocity</groupId>
      <artifactId>velocity</artifactId>
      <version>1.4-dev</version>
    </dependency>

    <dependency>
      <groupId>commons-jelly</groupId>
      <artifactId>commons-jelly-tags-velocity</artifactId>
      <version>20030303.205659</version>
    </dependency>

  </dependencies>

  <reports>
    <report>maven-license-plugin</report>
    <report>maven-faq-plugin</report>
    <report>maven-changes-plugin</report>
  </reports>

</project>
